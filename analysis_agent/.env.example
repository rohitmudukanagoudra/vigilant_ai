# API Keys (Required)
GEMINI_API_KEY=your_gemini_api_key_here

# Model Configuration
GEMINI_MODEL=gemini-2.5-flash
GEMINI_VISION_MODEL=gemini-2.5-flash

# Video Processing
FRAME_EXTRACTION_INTERVAL=2
MAX_FRAMES_PER_VIDEO=50

# OCR Settings
OCR_LANGUAGES=en
OCR_CONFIDENCE_THRESHOLD=0.3

# Vision Analysis
VISION_MAX_CONCURRENT=3
VISION_BATCH_SIZE=5
VISION_ANALYZE_ALL_FRAMES=true

# Agent Configuration
AGENT_TEMPERATURE=0.1
AGENT_MAX_RETRIES=3

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Streamlit Configuration
STREAMLIT_PORT=8501

# Logging
LOG_LEVEL=INFO